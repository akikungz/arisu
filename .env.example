# General
# Example: development
NODE_ENV=development
# Example: 3000
PORT=3000

# Secrets
# Example: change-me-32-chars-minimum
JWT_SECRET=change-me-32-chars-minimum
# Example: https://auth.example.com
BETTER_AUTH_URL=https://auth.example.com
# Example: 1234567890-abcdefghijklmnopqrstuvwxyz.apps.googleusercontent.com
GOOGLE_CLIENT_ID=your-google-client-id
# Example: GOCSPX-xxxxxxxxxxxxxxxxxxxx
GOOGLE_CLIENT_SECRET=your-google-client-secret
# Example: http://localhost:3000,https://app.example.com
ALLOW_CORS_ORIGINS=*

# Database
# Example: postgresql://user:password@localhost:5432/arisu
DATABASE_URL=postgresql://user:password@localhost:5432/arisu
# Example: redis://localhost:6379
REDIS_URL=redis://localhost:6379

# Telemetry
# Example: momoi
OTEL_SERVICE_NAME=momoi
# Example: http://localhost:4317
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
# Example: info
LOG_LEVEL=info
# Example: plain
LOG_FORMAT=plain
# Example: false
LOG_PRETTY=false
# Example: http://localhost:3100
LOG_LOKI_ENDPOINT=http://localhost:3100
